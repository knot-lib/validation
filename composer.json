{
  "name": "knot-lib/validation",
  "description": "Validation library for kNot Framework.",
  "keywords": ["PHP","Validation","kNot Framework"],
  "homepage": "https://github.com/knot-lib/validation",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@sazysoft.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "knotlib\\validation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "knotlib\\validation\\test\\": "test/",
      "knotlib\\validation\\test\\classes\\": "test/classes/"
    }
  }
}
